			</information>
			<delete>刪除此主題。</delete>
		</theme>
		<start_screen name="開始畫面">
			<image>圖像</image>
		</start_screen>
		<home_screen_backgrounds>主頁畫面的背景</home_screen_backgrounds>
	</theme_background>
	<date_time name="日期&時間">
