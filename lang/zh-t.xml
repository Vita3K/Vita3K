		<time_format name="時間顯示方式">
			<clock_12_hour>12小時制</clock_12_hour>
			<clock_24_hour>24小時制</clock_24_hour>
		</time_format>
	</date_time>
	<language name="語言">
		<system_language>系統語言</system_language>
	</language>
</settings>

